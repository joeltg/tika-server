{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "tika",
			"image": "logicalspark/docker-tikaserver",
			"essential": true,
			"memoryReservation": 1024,
			"portMappings": [
				{
					"hostPort": 9998,
					"containerPort": 9998
				}
			]
		},
		{
			"name": "server",
			"image": "joeltg/tika-server",
			"essential": true,
			"memoryReservation": 512,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 8080
				}
			]
		}
	],
	"volumes": []
}
