{
	"AWSEBDockerrunVersion": 2,
	"containerDefinitions": [
		{
			"name": "tika",
			"image": "logicalspark/docker-tikaserver",
			"command": [
				"-enableUnsecureFeatures",
				"-enableFileUrl",
				"-spawnChild",
				"-JXmx1g",
				"-JXms256m"
			],
			"essential": true,
			"memoryReservation": 1280
		},
		{
			"name": "server",
			"image": "joeltg/tika-server",
			"essential": true,
			"memoryReservation": 512,
			"links": ["tika"],
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 8080
				}
			]
		}
	],
	"volumes": []
}
